{
  "name": "dynamic-enterprise-demo",
  "version": "1.0.0",
  "description": "Enterprise demo showcasing Dynamic.xyz USDC wallet integration with account abstraction and secure authentication",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tunnel": "npx localtunnel --port 3000",
    "tunnel-ngrok": "ngrok http 3000 --host-header=\"localhost:3000\"",
    "dev-tunnel": "concurrently \"npm run dev\" \"sleep 5 && npm run tunnel\"",
    "dev-ngrok": "concurrently \"npm run dev\" \"sleep 3 && ngrok http 3000 --host-header=\\\"localhost:3000\\\"\"",
    "dev-mobile": "npx ngrok http 3000 --host-header=\"localhost:3000\" --log stdout",
    "get-ngrok-url": "curl -s http://localhost:4040/api/tunnels | python3 -c \"import sys, json; data=json.load(sys.stdin); print(data['tunnels'][0]['public_url'] if data.get('tunnels') else 'No active tunnels')\" 2>/dev/null || echo 'ngrok not running or API not accessible'",
    "test-prod": "npm run build && npm run start",
    "deploy": "vercel --prod",
    "deploy-preview": "vercel"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.3",
    "@dynamic-labs/bitcoin": "^4.51.2",
    "@dynamic-labs/cosmos": "^4.51.2",
    "@dynamic-labs/ethereum": "^4.51.2",
    "@dynamic-labs/ethereum-aa": "^4.51.2",
    "@dynamic-labs/iconic": "^4.51.2",
    "@dynamic-labs/sdk-api": "^0.0.830",
    "@dynamic-labs/sdk-react-core": "^4.51.2",
    "@dynamic-labs/solana": "^4.51.2",
    "@dynamic-labs/sui": "^4.51.2",
    "@dynamic-labs/types": "^4.51.2",
    "@dynamic-labs/wagmi-connector": "^4.51.2",
    "@dynamic-labs/zerodev-extension": "^4.51.2",
    "@lifi/sdk": "^3.13.5",
    "@tanstack/react-query": "^5.84.2",
    "jose": "^6.0.12",
    "lucide-react": "^0.474.0",
    "next": "15.5.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "viem": "^2.33.3",
    "wagmi": "^2.16.2"
  },
  "overrides": {
    "viem": "^2.33.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
